private static double Divide(double num1, double num2)
        {
            if (num2 != 0)
            {
                return num1 / num2;
            }
            else
            {
                throw new ArgumentException("You can't divide by 0");

            }
        }

        private static double Multiply(double num1, double num2)
        {
            return num1 * num2;
        }
